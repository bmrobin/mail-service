{
  "name": "mail-subscriber",
  "version": "0.0.0",
  "description": "Description for mail-subscriber",
  "appPath": "src/",
  "scripts": {
    "build-client": "webpack --config webpack.client.config.js",
    "build-server": "webpack --config webpack.server.config.js",
    "build": "npm run build-client && npm run build-server",
    "client": "webpack-dev-server --config webpack.client.config.js",
    "server": "npm run build-server && node src/dist/server",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "1.17.1",
    "bootstrap": "^3.3.7",
    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "4.15.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "jshint-stylish": "^2.1.0",
    "load-grunt-tasks": "^3.4.1",
    "lokijs": "^1.4.3",
    "nodemailer": "4.0.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.1",
    "react-dom": "^15.6.1",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.18.2",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.9",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.6.1"
  },
  "jest": {
    "cache": false,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/app/server/models/**/*.js",
      "src/app/server/services/**/*.js",
      "!src/dist/**"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "jsx",
      "js"
    ]
  },
  "engines": {
    "node": "^4.3"
  }
}
